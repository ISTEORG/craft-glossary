{
  "name": "isteorg/craft-glossary",
  "description": "Add glossaries with tooltips to Craft CMS.",
  "version": "3.0.0",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "glossary"
  ],
  "license": "proprietary",
  "authors": [
    {
      "name": "codemonauts",
      "homepage": "https://codemonauts.com"
    },
    {
      "name": "isteorg",
      "homepage": "https://iste.org"
    }
  ],
  "support": {
    "source": "https://github.com/isteorg/craft-glossary",
    "docs": "https://github.com/isteorg/craft-glossary/blob/main/README.md",
    "issues": "https://github.com/isteorg/craft-glossary/issues"
  },
  "require": {
    "craftcms/cms": "^4.0.0 || ^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "codemonauts\\glossary\\": "src/"
    }
  },
  "extra": {
    "handle": "glossary",
    "class": "codemonauts\\glossary\\Glossary",
    "name": "Glossary"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "craftcms/rector": "dev-main"
  }
}
